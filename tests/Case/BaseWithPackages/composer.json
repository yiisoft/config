{
    "name": "test/base-with-packages",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "require": {
        "yiisoft/config": "*",
        "test/a": "*"
    },
    "extra": {
        "config-plugin": {
            "params": "params.php",
            "web": []
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/config": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../.."
        },
        {
            "type": "path",
            "url": "packages/a"
        },
        {
            "type": "path",
            "url": "../../../vendor/yiisoft/arrays",
            "options": {
                "versions": {
                    "yiisoft/arrays": "3.0"
                }
            }
        },
        {
            "type": "path",
            "url": "../../../vendor/yiisoft/var-dumper",
            "options": {
                "versions": {
                    "yiisoft/var-dumper": "1.1"
                }
            }
        },
        {
            "type": "path",
            "url": "../../../vendor/yiisoft/strings",
            "options": {
                "versions": {
                    "yiisoft/strings": "2.1"
                }
            }
        }
    ]
}
